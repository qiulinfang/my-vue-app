{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~locales/*": [
                "src/locales/*"
            ],
            "~plugins/*": [
                "src/plugins/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~services/*": [
                "src/services/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~styles/*": [
                "src/styles/*"
            ],
            "~test/*": [
                "src/test/*"
            ],
            "~utils/*": [
                "src/utils/*"
            ],
            "~views/*": [
                "src/views/*"
            ],
            "~/*": [
                "src/*"
            ]
        }
    }
}